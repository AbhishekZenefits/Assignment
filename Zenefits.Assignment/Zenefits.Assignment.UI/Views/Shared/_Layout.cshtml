<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <link rel="dns-prefetch" href="//ajax.googleapis.com" />
    <meta name="keywords" content="" />
    <meta name="description" content="" />
    <meta name="viewport" content="width=device-width" />
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,300i,400,400i|Varela+Round" rel="stylesheet" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
    @Styles.Render("~/Content/css")
</head>
<body class="design">
    <div class="header-page-wrap">
        <header class="center-container-page">
            <div class="main-header">Nisha Test Company</div>
        </header>
    </div>
    <div class="mobile-navigation-wrap">
        <div class="mobile-navigation">
            <a href="javascript:void(0);" class="btn-nav-practice" data-target="nav-practice"><span class="fa fa-bars" aria-hidden="true"> </span>View Our Company Details</a>
            <div class="practice-area-mobile" data-set="mav-practice"></div>
        </div>
    </div>
    <div class="container-columns">
        @Html.Action("LeftBanner", "MyCompany")
        @RenderBody()
        
    </div>

    
    @Scripts.Render("~/bundles/scripts")
    @RenderSection("scripts", required: false)
</body>
</html>
